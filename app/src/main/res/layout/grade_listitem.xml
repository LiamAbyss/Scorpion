<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/grey_background">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/course_list_item_bottom_padding"
        android:layout_marginBottom="@dimen/course_list_item_bottom_padding"
        android:background="@drawable/white_round_corners_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/list_padding"
            android:layout_marginTop="@dimen/button_separator"
            android:text="Date"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <FrameLayout
            android:id="@+id/frameLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/button_separator"
            android:layout_marginTop="@dimen/button_separator"
            android:background="@color/colorContrast"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            tools:layout_editor_absoluteX="64dp">

        </FrameLayout>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Code"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/gradeCodeTextView"
            app:layout_constraintStart_toStartOf="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

        <FrameLayout
            android:id="@+id/frameLayout2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/button_separator"
            android:layout_marginTop="@dimen/button_separator"
            android:background="@color/colorContrast"
            app:layout_constraintTop_toBottomOf="@+id/gradeCodeTextView">

        </FrameLayout>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Libellé"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/gradeLibelleTextView"
            app:layout_constraintStart_toStartOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/gradeLibelleTextView" />

        <FrameLayout
            android:id="@+id/frameLayout3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/button_separator"
            android:layout_marginTop="@dimen/button_separator"
            android:background="@color/colorContrast"
            app:layout_constraintTop_toBottomOf="@+id/gradeLibelleTextView"
            tools:layout_editor_absoluteX="135dp">

        </FrameLayout>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_separator"
            android:text="Note"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout3" />

        <FrameLayout
            android:id="@+id/frameLayout4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/button_separator"
            android:layout_marginTop="@dimen/button_separator"
            android:background="@color/colorContrast"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            tools:layout_editor_absoluteX="166dp">

        </FrameLayout>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Motif d'absence"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/gradeReasonTextView"
            app:layout_constraintStart_toStartOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="@+id/gradeReasonTextView" />

        <FrameLayout
            android:id="@+id/frameLayout5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/button_separator"
            android:layout_marginTop="@dimen/button_separator"
            android:background="@color/colorContrast"
            app:layout_constraintTop_toBottomOf="@+id/gradeReasonTextView"
            tools:layout_editor_absoluteX="159dp">

        </FrameLayout>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Appréciation"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/gradeAppreciationTextView"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/gradeAppreciationTextView" />

        <FrameLayout
            android:id="@+id/frameLayout6"
            android:layout_width="match_parent"
            android:layout_height="@dimen/button_separator"
            android:layout_marginTop="@dimen/button_separator"
            android:background="@color/colorContrast"
            app:layout_constraintTop_toBottomOf="@+id/gradeAppreciationTextView"
            tools:layout_editor_absoluteX="128dp">

        </FrameLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Intervenants"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/gradeTeachersTextView"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintTop_toTopOf="@+id/gradeTeachersTextView" />

        <FrameLayout
            android:id="@+id/frameLayout7"
            android:layout_width="@dimen/button_separator"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/list_padding"
            android:background="@color/colorContrast"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="parent"
            tools:visibility="visible">

        </FrameLayout>

        <TextView
            android:id="@+id/gradeDateTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/list_padding"
            android:text="DD/MM/AAAA"
            app:layout_constraintStart_toEndOf="@+id/frameLayout7"
            app:layout_constraintTop_toTopOf="@+id/textView" />

        <TextView
            android:id="@+id/gradeCodeTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_separator"
            android:text="/"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/gradeDateTextView"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

        <TextView
            android:id="@+id/gradeLibelleTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_separator"
            android:layout_marginEnd="@dimen/button_separator"
            android:text="/"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/gradeCodeTextView"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout2" />

        <TextView
            android:id="@+id/gradeNoteTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="/"
            app:layout_constraintStart_toStartOf="@+id/gradeLibelleTextView"
            app:layout_constraintTop_toTopOf="@+id/textView4" />

        <TextView
            android:id="@+id/gradeReasonTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_separator"
            android:text="@string/empty_grade"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/gradeNoteTextView"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout4" />

        <TextView
            android:id="@+id/gradeAppreciationTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_separator"
            android:text="@string/empty_grade"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/gradeReasonTextView"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout5" />

        <TextView
            android:id="@+id/gradeTeachersTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_separator"
            android:layout_marginBottom="@dimen/button_separator"
            android:text="@string/empty_grade"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/gradeAppreciationTextView"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout6" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>